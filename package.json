{
  "name": "react-project-boilerplate",
  "version": "1.0.0",
  "description": "greatest boilerplate you can ever find",
  "main": "src/index.js",
  "private": "true",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --collect-coverage",
    "postinstall": "husky install",
    "make-badges": "istanbul-badges-readme",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && istanbul-badges-readme && git add 'README.md'"
    }
  },
  "repository": {
    "url": "https://github.com/Bukenov-s/react-project-boilerplate"
  },
  "keywords": [],
  "author": "serik",
  "license": "ISC",
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.2",
    "@sentry/react": "^6.13.3",
    "@sentry/tracing": "^6.13.3",
    "axios": "0.21.2",
    "bootstrap": "^5.1.3",
    "i18next": "^21.8.10",
    "i18next-browser-languagedetector": "^6.1.2",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.4.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.32.2",
    "react-i18next": "^11.17.2",
    "react-redux": "^8.0.2",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "redux": "^4.0.5",
    "redux-persist": "5.10.0",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@storybook/addon-actions": "^6.4.0-rc.1",
    "@storybook/addon-essentials": "^6.4.0-rc.1",
    "@storybook/addon-links": "^6.4.0-rc.1",
    "@storybook/react": "^6.4.0-rc.1",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.3",
    "@testing-library/user-event": "^12.0.11",
    "@types/jest": "^25.1.4",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "@vitejs/plugin-react": "^1.0.5",
    "babel-loader": "^8.2.3",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-storybook": "^0.4.1",
    "husky": "^5.0.6",
    "istanbul-badges-readme": "^1.2.0",
    "jest": "^25.1.0",
    "prettier": "^2.1.2",
    "storybook-builder-vite": "^0.1.6",
    "ts-jest": "^25.2.1",
    "ts-loader": "^7.0.5",
    "typescript": "^4.4.4",
    "vite": "^2.6.10",
    "vite-plugin-pwa": "^0.12.0"
  }
}
